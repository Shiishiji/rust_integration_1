<?xml version="1.0" encoding="UTF-8" ?>
<interface>
    <template class="LaptopList" parent="GtkBox">
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkBox" id="list_header">
                <property name="valign">center</property>
                <property name="margin-start">1px</property>
                <child>
                    <object class="GtkLabel" id="manufacturer_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Producent</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="screen_size_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Wielkość matrycy</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="screen_resolution_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Rozdzielczość</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="screen_type_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Typ matrycy</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="screen_touchscreen_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Czy dotykowy ekran</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="processor_name_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Procesor</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="processor_physical_cores_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Liczba rdzeni fizycznych</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="processor_clock_speed_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Taktowanie</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="ram_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">RAM</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="disc_storage_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Pojemność dysku</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="disc_type_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Typ dysku</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="graphic_card_name_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Karta graficzna</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="graphic_card_memory_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Pamięć karty graficznej</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="os_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">System operacyjny</property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="disc_reader_header_label">
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="label">Napęd optyczny</property>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListView" id="list">
                <property name="valign">start</property>
            </object>
        </child>
    </template>
</interface>